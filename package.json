{
    "name":  "sigesalud-demo",
    "version":  "0.1.0",
    "private":  true,
    "main":  "dist/main/main.js",
    "scripts":  {
                    "dev":  "concurrently \"powershell -NoProfile -Command \\\"Set-Location src/renderer; npx vite --port 5175 --strictPort\\\"\" \"npx tsc -w\" \"powershell -NoProfile -Command \\\"$env:ELECTRON_RUN_AS_NODE=''; $env:NODE_ENV='development'; npx wait-on http://localhost:5175; npx electron .\\\"\"",
                    "dev:renderer":  "vite",
                    "dev:compile":  "tsc --project tsconfig.json",
                    "build":  "electron-vite build",
                    "start":  "electron .",
                    "seed":  "node ./src/main/seed/seed.js",
                    "seed:hr":  "node ./src/main/seed/generate-hr.js"
                },
    "devDependencies":  {
                            "@types/better-sqlite3":  "^7.4.2",
                            "@types/node":  "^20.0.0",
                            "@types/react":  "^18.0.0",
                            "@types/react-dom":  "^18.0.0",
                            "@vitejs/plugin-react":  "^4.2.1",
                            "concurrently":  "^8.0.0",
                            "electron-vite":  "^2.0.0",
                            "ts-node":  "^10.0.0",
                            "typescript":  "^5.0.0",
                            "vite":  "^5.0.0",
                            "wait-on":  "^9.0.3"
                        },
    "dependencies":  {
                         "bcryptjs":  "^2.4.3",
                         "electron":  "^27.0.0",
                         "lowdb":  "^6.0.1",
                         "nanoid":  "^4.0.0",
                         "react":  "^18.2.0",
                         "react-dom":  "^18.2.0",
                         "react-router-dom":  "^6.0.0",
                         "sql.js":  "^1.10.2"
                     }
}
